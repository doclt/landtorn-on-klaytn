{
  "name": "landtorn",
  "version": "0.1",
  "type": "module",
  "description": "Landtorn core",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.12"
  },
  "author": "Landtorn",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc",
    "lint": "DEBUG=eslint:cli-engine npx eslint 'src/**/*.ts' 'src/*.ts'",
    "start:listener": "node --no-warnings --experimental-specifier-resolution=node --experimental-json-modules dist/listener/main",
    "start:listener:torn": "yarn start:listener --service TORN",
    "start:worker": "node --no-warnings --experimental-specifier-resolution=node --experimental-json-modules dist/worker/main.js",
    "start:worker:torn": "yarn start:worker --worker TORN",
    "flush": "node --no-warnings --experimental-specifier-resolution=node --experimental-json-modules dist/tools/flush-queues.js",
    "start:listener:claimer": "yarn start:listener --service CLAIMER",
    "start:worker:claimer": "yarn start:worker --worker CLAIMER",
    "start:listener:sacrifice": "yarn start:listener --service SACRIFICE",
    "start:worker:sacrifice": "yarn start:worker --worker SACRIFICE",
    "start:listener:reward": "yarn start:listener --service REWARD",
    "start:worker:reward": "yarn start:worker --worker REWARD",
    "start:listener:shard": "yarn start:listener --service SHARD",
    "start:worker:shard": "yarn start:worker --worker SHARD",
    "start:listener:market": "yarn start:listener --service MARKETPLACE",
    "start:worker:market": "yarn start:worker --worker MARKETPLACE"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.9.1",
    "axios": "^1.4.0",
    "bullmq": "^4.7.0",
    "dotenv": "^16.3.1",
    "eslint": "8.52.0",
    "ethers": "^6.7.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "redis": "^4.6.7"
  }
}
